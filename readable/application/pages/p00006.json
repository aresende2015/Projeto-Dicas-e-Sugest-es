   ,{
      "id":"68501090967536207819"
     ,"Identification":{
        "Name":"Valida o Login"
      }
     ,"Execution":{
        "Sequence":10
      }
     ,"Validation":{
        "Type":"PL\/SQL Error"
       ,"PL\/SQL Code raising Error":"declare\n    retorno number;\nbegin\n\n    select count(1) into retorno from APEX_APPL_ACL_USERS where USER_NAME = trim(upper(:P6_LOGIN));\n\n    if retorno \u003E 0 then\n        raise_application_error( -20000, \u0027Usu\u00E1rio \u0027 || :P6_LOGIN || \u0027 j\u00E1 existe.\u0027 );\n    else\n        select count(1) into retorno from TDOW_USUARIO where trim(upper(LOGIN)) = trim(upper(:P6_LOGIN));\n        if retorno \u003E 0 then\n            raise_application_error( -20001, \u0027Usu\u00E1rio \u0027 || :P6_LOGIN || \u0027 j\u00E1 foi usado.\u0027 );\n        end if;\n    end if;\n\nend;"
       ,"Always Execute":false
      }
